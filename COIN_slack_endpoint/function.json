{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "coinbot",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "records",
      "databaseName": "kogoe-cosmos-db",
      "collectionName": "kogoe",
      "connectionStringSetting": "DB_CONN_STRING",
      "sqlQuery": "SELECT * FROM c "
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "authUsers",
      "databaseName": "kogoe-cosmos-db",
      "collectionName": "kogoe",
      "connectionStringSetting": "DB_CONN_STRING",
      "sqlQuery": "SELECT c.id FROM c where c.authorized = true"
    }
  ],
  "scriptFile": "../dist/lib/functions.js",
  "entryPoint": "coinInitTrigger"
}